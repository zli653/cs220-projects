    "def abs(list):\n",
    "    list = list[:] # copy it\n",
    "    for i in range(len(list)):\n",
    "        if list[i] < 0:\n",
    "            list[i] = -list[i]\n",
    "    return list\n",
    "\n",
    "abs([-1, -3, 5, -4, 8])"
   ]
